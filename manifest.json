{
  "name": "You Lack Discipline!",
  "version": "2.1",
  "description": "The extension that keeps you from looking at those distracting sites that keep you from doing what you need to do.",
  "manifest_version": 2,
  "background": {
  		"page": "background.html",
  		"persistent": false
  		},
  "browser_action": {
    "default_icon": "images/icon16.png",
    "default_popup":"popup.html"
  },
  "icons":{
  		"16" : "images/icon16.png",
  		"48" : "images/icon48.png",
  		"128": "images/icon128.png"
  },
  "permissions": ["tabs"]
}